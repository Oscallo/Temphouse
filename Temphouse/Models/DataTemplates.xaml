<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Temphouse.Models"
                    xmlns:fa="http://schemas.awesome.incremented/wpf/xaml/fontawesome.sharp">


    <DataTemplate DataType="{x:Type local:MenuButtonModel}">
        <fa:IconImage Margin="4" VerticalAlignment="Center" HorizontalAlignment="Center" Icon="{Binding Icon}" Foreground="{Binding Foreground,RelativeSource={RelativeSource AncestorType=ContentControl}}"/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type local:UserWithoutPasswordModel}">
        <Grid Height="50" Width="50" Margin="5,10,5,10" Background="#4C206D77"/>
    </DataTemplate>

</ResourceDictionary>