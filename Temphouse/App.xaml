             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Temphouse"
             StartupUri="/Windows/DashboardWindow.xaml" >
    <Application.Resources>

        <ResourceDictionary>
            
            <!-- Следующая строка исправляет MS ошибку -->
            <Style TargetType="{x:Type Rectangle}" />
            <!-- Предыдущая строка исправляет MS ошибку -->

            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/Temphouse;component/Visualize/Generic.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>


    </Application.Resources>
</Application>
